<h1>Basic Info</h1>
<%= form_with model: @profile_form, url: profile_path(@profile), method: :patch do |f| %>
  <div>
    <%= f.label :first_name %>
    <%= f.text_field :first_name %>
  </div>
  <div>
    <%= f.label :last_name %>
    <%= f.text_field :last_name %>
  </div>
  <div>
    <%= f.label :name %>
    <%= f.text_field :name %>
  </div>
  <div>
    <%= f.label :title %>
    <%= f.text_field :title %>
  </div>
  <div>
    <%= f.label :years_of_experience %>
    <%= f.text_field :years_of_experience %>
  </div>
  <div>
    <%= f.label :photo %>
    <% if @profile_form.photo.present? %>
      <%= image_tag @profile_form.photo_url %>
    <% end %>
    <%= f.file_field :photo %>
  </div>
  <div>
    <%= f.label :rate %>
    <%= f.text_field :rate %>
  </div>
  <div>
    <%= f.label :expercoin_rate %>
    <%= f.text_field :expercoin_rate %>
  </div>
  <div>
    <%= f.label :address %>
    <%= f.text_field :address %>
  </div>
  <div>
    <%= f.label :county %>
    <%= f.text_field :country %>
  </div>
  <div>
    <%= f.label :state %>
    <%= f.text_field :state %>
  </div>
  <div>
    <%= f.label :city %>
    <%= f.text_field :city %>
  </div>
  <div>
    <label>Categories</label>
    <%= f.collection_select :category_ids, @categories, :id, :name, {}, { class: 'selectpicker', 'data-live-search':"true" } %>
  </div>

  <%= f.submit %>
<% end %>
